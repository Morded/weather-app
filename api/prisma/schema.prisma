// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Weather {
  id        Int       @default(autoincrement()) @id
  queriedAt DateTime  @default(now())
  city      String
  lat       Float
  lon       Float
  temp      Float
  pressure  Float
  humidity  Float 
  windDeg   Float 
  windSpeed Float
  windGust  Float
  cloud     Float 
  rain      Float
}
